else{\n                this.page-=1;\n                this.getitemFuction(this.page);\n            }\n        },\n        pageNumber(num){\n            if(num <=1){\n                this.page = 1;\n                this.getitemFuction(this.page);\n            }else if(num >= this.last_page){\n                this.page = this.last_page;\n                this.getitemFuction(this.page);\n            }else{\n                this.page = num;\n                this.getitemFuction(this.page);\n            }\n        }\n    }\n\n}\n</script>\n\n<style>\n\n</style>"],"sourceRoot":"src/components/pagination"},"metadata":{},"sourceType":"module"}