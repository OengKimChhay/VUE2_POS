s.discount / 100);\n    },\n    ITEM: function ITEM() {\n      return this.orderItems.reduce(function (acc, item) {\n        return acc + item.qty;\n      }, 0);\n    }\n  }),\n  methods: _objectSpread(_objectSpread({}, mapActions('dashboard', ['getAllProduct', 'getAllCategory', 'getAllTable'])), {}, {\n    refreshPage: function refreshPage() {\n      window.location.reload();\n    },\n    selectCat: function selectCat(catID) {\n      this.category_id = catID;\n      this.showCatList = false;\n      this.showProList = true;\n    },\n    Back: function Back() {\n      this.showCatList = true;\n      this.showProList = false;\n      this.showTable = false;\n      this.showPromoTable = false;\n    },\n    Table: function Table() {\n      this.showTable = true;\n      this.showCatList = false;\n      this.showProList = false;\n      this.showPromoTable = false;\n    },\n    promoTion: function promoTion() {\n      this.showPromoTable = true;\n      this.showTable = false;\n      this.showCatList = false;\n      this.showProList = false;\n    },\n    TakeDiscount: function TakeDiscount(dis, rat) {\n      // dis and rat are take from prop\n      this.discount = dis;\n      this.rate = rat;\n    },\n    selectTab: function selectTab(table) {\n      this.table = table.name;\n    },\n    TakeOrder: function TakeOrder(product) {\n      var index = this.orderItems.findIndex(function (item) {\n        return item.id === product.id;\n      });\n\n      if (index != -1) {\n        //check if item inorderItems array is the same with item that user just add (=-1 mean same item)\n        this.orderItems[index].qty++; //if same increase only qty and price in that index of item\n      } else {\n        var order = {\n          id: product.id,\n          name: product.product_name,\n          price: product.unitprice,\n          qty: 1\n        };\n        this.orderItems.push(order);\n      }\n    },\n    deleteOrder: function deleteOrder(index) {\n      this.orderItems.splice(index, 1); //1 mean that remove 1 item\n    }\n  }),\n  mounted: function mounted() {\n    //call below mehtod from ...mapActions if  we not mouted category will not found\n    this.getAllProduct(); //call this mehtod from ...mapActions if  we not mouted users will not found\n\n    this.getAllCategory();\n    this.getAllTable();\n  }\n};","map":{"version":3,"sources":["Pos.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAiHA,OAAA,KAAA,MAAA,0BAAA,C,CAAA;;AACA,SAAA,UAAA,EAAA,UAAA,QAAA,MAAA;AACA,OAAA,KAAA,MAAA,0BAAA,C,CACA;;AACA,OAAA,UAAA,MAAA,iBAAA;AACA,OAAA,QAAA,MAAA,oBAAA;AACA,OAAA,OAAA,MAAA,mBAAA;AACA,OAAA,SAAA,MAAA,qBAAA;AACA,eAAA;AACA,EAAA,MAAA,EAAA,CAAA,KAAA,CADA;AAEA,EAAA,IAAA,EAAA,KAFA;AAGA,EAAA,UAAA,EAAA;AACA,IAAA,KAAA,EAAA,KADA;AAEA,IAAA,UAAA,EAAA,UAFA;AAGA,IAAA,QAAA,EAAA,QAHA;AAIA,IAAA,OAAA,EAAA,OAJA;AAKA,IAAA,SAAA,EAAA;AALA,GAHA;AAUA,EAAA,IAVA,kBAUA;AACA,WAAA;AACA,MAAA,KAAA,EAAA,EADA;AAEA,MAAA,WAAA,EAAA,EAFA;AAGA,MAAA,WAAA,EAAA,IAHA;AAIA,MAAA,WAAA,EAAA,KAJA;AAKA,MAAA,SAAA,EAAA,KALA;AAMA,MAAA,cAAA,EAAA,KANA;AAOA,MAAA,UAAA,EAAA,EAPA;AAQA,MAAA,QAAA,EAAA,CARA;AASA,MAAA,IAAA,EAAA;AATA,KAAA;AAWA,GAtBA;AAuBA,EAAA,QAAA,kCACA,UAAA,CAAA,WAAA,EAAA,CAAA,aAAA,EAAA,eAAA,EAAA,WAAA,CAAA,CADA;AAGA,IAAA,KAHA,mBAGA;AAAA;AACA,aAAA,KAAA,UAAA,CAAA,MAAA,CAAA,UAAA,GAAA,EAAA,IAAA;AAAA,eAAA,GAAA,GAAA,UAAA,CAAA,IAAA,CAAA,KAAA,GAAA,IAAA,CAAA,GAAA,CAAA;AAAA,OAAA,EAAA,CAAA,CAAA;AACA,KALA;AAMA,IAAA,WANA,yBAMA;AACA,aAAA,KAAA,KAAA,GAAA,KAAA,KAAA,IAAA,KAAA,QAAA,GAAA,GAAA,CAAA;AACA,KARA;AASA,IAAA,IATA,kBASA;AACA,aAAA,KAAA,UAAA,CAAA,MAAA,CAAA,UAAA,GAAA,EAAA,IAAA;AAAA,eAAA,GAAA,GAAA,IAAA,CAAA,GAAA;AAAA,OAAA,EAAA,CAAA,CAAA;AACA;AAXA,IAvBA;AAoCA,EAAA,OAAA,kCACA,UAAA,CAAA,WAAA,EAAA,CAAA,eAAA,EAAA,gBAAA,EAAA,aAAA,CAAA,CADA;AAEA,IAAA,WAFA,yBAEA;AACA,MAAA,MAAA,CAAA,QAAA,CAAA,MAAA;AACA,KAJA;AAKA,IAAA,SALA,qBAKA,KALA,EAKA;AACA,WAAA,WAAA,GAAA,KAAA;AACA,WAAA,WAAA,GAAA,KAAA;AACA,WAAA,WAAA,GAAA,IAAA;AACA,KATA;AAUA,IAAA,IAVA,kBAUA;AACA,WAAA,WAAA,GAAA,IAAA;AACA,WAAA,WAAA,GAAA,KAAA;AACA,WAAA,SAAA,GAAA,KAAA;AACA,WAAA,cAAA,GAAA,KAAA;AACA,KAfA;AAgBA,IAAA,KAhBA,mBAgBA;AACA,WAAA,SAAA,GAAA,IAAA;AACA,WAAA,WAAA,GAAA,KAAA;AACA,WAAA,WAAA,GAAA,KAAA;AACA,WAAA,cAAA,GAAA,KAAA;AACA,KArBA;AAsBA,IAAA,SAtBA,uBAsBA;AACA,WAAA,cAAA,GAAA,IAAA;AACA,WAAA,SAAA,GAAA,KAAA;AACA,WAAA,WAAA,GAAA,KAAA;AACA,WAAA,WAAA,GAAA,KAAA;AACA,KA3BA;AA4BA,IAAA,YA5BA,wBA4BA,GA5BA,EA4BA,GA5BA,EA4BA;AACA;AACA,WAAA,QAAA,GAAA,GAAA;AACA,WAAA,IAAA,GAAA,GAAA;AACA,KAhCA;AAiCA,IAAA,SAjCA,qBAiCA,KAjCA,EAiCA;AACA,WAAA,KAAA,GAAA,KAAA,CAAA,IAAA;AACA,KAnCA;AAoCA,IAAA,SApCA,qBAoCA,OApCA,EAoCA;AACA,UAAA,KAAA,GAAA,KAAA,UAAA,CAAA,SAAA,CAAA,UAAA,IAAA;AAAA,eAAA,IAAA,CAAA,EAAA,KAAA,OAAA,CAAA,EAAA;AAAA,OAAA,CAAA;;AACA,UAAA,KAAA,IAAA,CAAA,CAAA,EAAA;AAAA;AACA,aAAA,UAAA,CAAA,KAAA,EAAA,GAAA,GADA,CACA;AACA,OAFA,MAEA;AACA,YAAA,KAAA,GAAA;AACA,UAAA,EAAA,EAAA,OAAA,CAAA,EADA;AAEA,UAAA,IAAA,EAAA,OAAA,CAAA,YAFA;AAGA,UAAA,KAAA,EAAA,OAAA,CAAA,SAHA;AAIA,UAAA,GAAA,EAAA;AAJA,SAAA;AAMA,aAAA,UAAA,CAAA,IAAA,CAAA,KAAA;AACA;AACA,KAjDA;AAkDA,IAAA,WAlDA,uBAkDA,KAlDA,EAkDA;AACA,WAAA,UAAA,CAAA,MAAA,CAAA,KAAA,EAAA,CAAA,EADA,CACA;AACA;AApDA,IApCA;AA0FA,EAAA,OA1FA,qBA0FA;AACA;AACA,SAAA,aAAA,GAFA,CAEA;;AACA,SAAA,cAAA;AACA,SAAA,WAAA;AACA;AA/FA,CAAA","sourcesContent":["<template>\n    <div class=\"wrappe-order\">\n        <div class=\"go-back\">\n            <button>\n                <router-link :to=\"{ name: 'Dashboard' }\" style=\"text-decoration:none;padding:7px;color: #910101;\" ><b-icon icon=\"arrow-bar-left\"></b-icon>Exit</router-link>\n            </button>\n            <button @click=\"refreshPage\">\n                Refresh\n            </button>\n        </div>\n        <div class=\"wrapper\">\n            <section>\n                <p class=\"title\">Order</p>\n                <!-- ------------------take order-------------------->\n                <div class=\"order\">\n                    <div class=\"header-order\">\n                        <div class=\"cahier\">\n                            <p>Cashier Name: {{username | capitaLize}}</p>\n                            <p>Order Number: 0012</p>\n                            <p>Table:{{table}}</p>\n                        </div>\n                        <div class=\"date\">\n                            <Clock></Clock>\n                        </div>\n                    </div>\n                    <div class=\"body-order\">\n                        <table class=\"table-order\">\n                            <thead>\n                                <tr>\n                                    <th style=\"width:60px\">N0.</th>\n                                    <th>Pro_Name</th>\n                                    <th style=\"width:130px\">Price</th>\n                                    <th style=\"width:130px\">Qty</th>\n                                    <th style=\"width:130px\">Dis</th>\n                                    <th style=\"width:130px\">Action</th>\n                                </tr>\n                            </thead>\n                            <tbody>\n                                <tr v-for=\"(item,index) in orderItems\" :key=\"index\">\n                                    <td class=\"text-center\">{{index+1}}</td>\n                                    <td>{{item.name}}</td>\n                                    <td class=\"text-center\">{{item.price | toCurrency}}</td> <!--toCurrency is a filter pls see /filter/main.js-->\n                                    <td class=\"text-center\"><div class=\"qty-btn\"><input v-model=\"item.qty\" type=\"number\" min=\"1\" title=\"Quantity\"></div></td>\n                                    <td class=\"text-center\">{{discount}} %</td>\n                                    <td class=\"text-center\"><button @click=\"deleteOrder(index)\" title=\"Delete\" class=\"delete-order\"><b-icon icon=\"trash\" ></b-icon></button></td>\n                                </tr>\n                                <tr v-if=\"orderItems.length <= 0\">\n                                    <td style=\"text-align:center;padding:20px;\" colspan=\"6\">No item yet.</td>\n                                </tr>\n                            </tbody>\n                        </table>\n                        <div  class=\"order-detail\">\n                            <div>\n                                <p>Total: <span>{{TOTAL | toCurrency}}</span></p>\n                                <p>Rate: {{rate}}៛</p>\n                                <p>Dis: <span>{{discount}} %</span></p>\n                                <p>Amount to pay: <span>{{AMOUNTTOPAY | toCurrency}}</span> </p>\n                            </div>\n                            <div>\n                                <button v-b-modal.modal-1 class=\"order-button\" v-if=\"orderItems.length > 0\">Order</button>\n                            </div>\n        <!---------------------------------- show order detail alert -------------------------------------->\n                            <b-modal id=\"modal-1\" size=\"lg\" title=\"Order Detail\" hide-footer>\n                                <div class=\"body-order-detail d-flex justify-content-center w-100\">\n                                    <div class=\"w-50\">\n                                        <p>Item: {{ITEM}}</p>\n                                        <p>Total: {{TOTAL | toCurrency}}</p>\n                                        <p>Discount: {{discount}}%</p>\n                                        <p>Amount To Pay</p>  \n                                        <div class=\"d-flex\">\n                                            <p>{{AMOUNTTOPAY | toCurrency}}</p>\n                                            <p>{{AMOUNTTOPAY*rate | toCurrencyReil}}</p> \n                                        </div>\n                                    </div>\n                                    <div class=\"w-50\" style=\"border-left:2px solid #9c9c9c;\">\n                                        <div style=\"margin-left:20px;\">\n                                            <div><p>Reil(៛)</p><p>Dollar($)</p></div>\n                                            <div><input type=\"number\"><input type=\"number\"></div>\n                                        </div>\n                                    </div>\n                                </div>\n                                <hr>\n                                <div class=\"footer-order-detail\">\n                                    <b-button variant=\"success\">Order</b-button>\n                                </div>\n                            </b-modal>\n        <!----------------------------------end show order detail alert -------------------------------------->\n                        </div>\n                    </div>\n                </div>\n            </section>\n            <section>\n                <p class=\"title\">Menu</p>\n                <div class=\"menu-option\">\n                    <button v-on:click=\"orderItems=[]\">Clear Order</button>\n                    <button v-on:click=\"Table\">Table</button>\n                    <button v-on:click=\"promoTion\">Setting (%,rate)</button>\n                    <button v-if=\"showProList || showTable || showPromoTable\" @click=\"Back\" size=\"sm\"><b-icon icon=\"chevron-double-left\"></b-icon> Back</button>\n                </div>\n\n                <!-- we use prop(pass from parent to child) and $emit(pass child to parent) to pass data each other child~parent -->\n                <TableOrder v-if=\"showTable\" :table=\"this.Alltables\" @passTable=\"selectTab\"></TableOrder>\n                <!-- ------------------show category list--------------- -->\n                <Category v-if=\"showCatList\" :category=\"this.Allcategories\" @PassCatID=\"selectCat\"></Category> \n                <!-- ------------------product show base on category -->\n                <Product  v-if=\"showProList\" :product=\"this.Allproducts\" :category_id=\"this.category_id\" @PassProduct=\"TakeOrder\"></Product>\n                <Promotion v-if=\"showPromoTable\" @passDiscount=\"TakeDiscount\"></Promotion>\n            </section>\n        </div>\n    </div>\n</template>\n\n<script>\nimport Mixin from '../../../../helper/mixin'; //for use of username\nimport {mapActions,mapGetters} from 'vuex';\nimport Clock from '../../../../helper/clock';\n// -------components-------\nimport TableOrder from \"./listing/Table\";\nimport Category from './listing/Category';\nimport Product from './listing/Product';\nimport Promotion from './listing/Promotion';\nexport default {\n    mixins: [Mixin],\n    name:'Pos',\n    components:{\n        Clock, \n        TableOrder,\n        Category, \n        Product,\n        Promotion\n    },\n    data(){\n        return{\n            table:'',\n            category_id: '',\n            showCatList: true,\n            showProList: false,\n            showTable: false,\n            showPromoTable: false,\n            orderItems:[],\n            discount:0,\n            rate:4100,\n        }\n    },\n    computed:{\n        ...mapGetters('dashboard',['Allproducts','Allcategories','Alltables']),\n        \n        TOTAL(){ //to get amoute total of order\n            return this.orderItems.reduce((acc, item) => acc + parseFloat(item.price*item.qty), 0);\n        }, \n        AMOUNTTOPAY(){\n            return this.TOTAL - (this.TOTAL * (this.discount/100));\n        },\n        ITEM(){\n            return this.orderItems.reduce((acc, item) => acc + item.qty, 0);\n        }\n    },\n    methods:{\n        ...mapActions('dashboard',['getAllProduct','getAllCategory','getAllTable']),\n        refreshPage(){\n            window.location.reload();\n        },\n        selectCat(catID){\n            this.category_id = catID;\n            this.showCatList = false;\n            this.showProList = true;\n        },\n        Back(){\n            this.showCatList= true;\n            this.showProList= false;\n            this.showTable= false;\n            this.showPromoTable= false;\n        },\n        Table(){\n            this.showTable= true;\n            this.showCatList= false;\n            this.showProList= false;\n            this.showPromoTable= false;\n        },\n        promoTion(){\n            this.showPromoTable= true;\n            this.showTable= false;\n            this.showCatList= false;\n            this.showProList= false;\n        },\n        TakeDiscount(dis,rat){\n            // dis and rat are take from prop\n            this.discount = dis;\n            this.rate = rat;\n        },\n        selectTab(table){\n            this.table = table.name;\n        },\n        TakeOrder(product){\n            let index = this.orderItems.findIndex((item) => item.id === product.id); \n            if(index != -1){ //check if item inorderItems array is the same with item that user just add (=-1 mean same item)\n                this.orderItems[index].qty ++; //if same increase only qty and price in that index of item\n            }else{\n                let order = {\n                    id   : product.id,\n                    name : product.product_name,\n                    price: product.unitprice,\n                    qty  :1,\n                }\n                this.orderItems.push(order);\n            }\n        },\n        deleteOrder(index){\n            this.orderItems.splice(index, 1); //1 mean that remove 1 item\n        },\n    },\n    mounted(){\n        //call below mehtod from ...mapActions if  we not mouted category will not found\n        this.getAllProduct();   //call this mehtod from ...mapActions if  we not mouted users will not found\n        this.getAllCategory();\n        this.getAllTable(); \n    },\n}\n</script>\n\n<style>\n.body-order-detail div p{\n    font-size: 19px;\n    font-weight: 500;\n}\n.body-order-detail div > div > p{\n    font-size: 19px;\n    color: #bc0000;\n    font-weight: 500;\n    border: 1px solid #a2a2a2;\n    padding: 15px;\n    bac