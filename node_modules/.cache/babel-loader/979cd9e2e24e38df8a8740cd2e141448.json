{"ast":null,"code":"//\n//\n//\n//\n//\n//\n//\nexport default {\n  data: function data() {\n    return {\n      time: \"\",\n      date: \"\"\n    };\n  },\n  methods: {\n    showTime: function showTime() {\n      var date = new Date();\n      var h = date.getHours(); // 0 - 23\n\n      var m = date.getMinutes(); // 0 - 59\n\n      var s = date.getSeconds(); // 0 - 59\n\n      var months = ['Jan', 'Feb', 'Mar', 'Apr', 'May', 'Jun', 'Jul', 'Aug', 'Sep', 'Oct', 'Nov', 'Dec'];\n      var days = ['Sun', 'Mon', 'Tue', 'Wed', 'Thu', 'Fri', 'Sat']; // ------------time--------------\n\n      var session = \"AM\";\n\n      if (h == 0) {\n        h = 12;\n      }\n\n      if (h > 12) {\n        h = h - 12;\n        session = \"PM\";\n      }\n\n      h = h < 10 ? \"0\" + h : h;\n      m = m < 10 ? \"0\" + m : m;\n      s = s < 10 ? \"0\" + s : s;\n      this.time = h + \":\" + m + \":\" + s + \" \" + session; // -----------date--------------\n\n      var curWeekDay = days[date.getDay()];\n      var curDay = date.getDate();\n      var curMonth = months[date.getMonth()];\n      var curYear = date.getFullYear();\n      this.date = curWeekDay + \"/\" + curDay + \"/\" + curMonth + \"/\" + curYear;\n      setTimeout(this.showTime, 1000);\n    }\n  },\n  mounted: function mounted() {\n    this.showTime();\n  }\n};","map":{"version":3,"sources":["clock.vue"],"names":[],"mappings":";;;;;;;AAQA,eAAA;AACA,EAAA,IADA,kBACA;AACA,WAAA;AACA,MAAA,IAAA,EAAA,EADA;AAEA,MAAA,IAAA,EAAA;AAFA,KAAA;AAIA,GANA;AAOA,EAAA,OAAA,EAAA;AACA,IAAA,QADA,sBACA;AACA,UAAA,IAAA,GAAA,IAAA,IAAA,EAAA;AACA,UAAA,CAAA,GAAA,IAAA,CAAA,QAAA,EAAA,CAFA,CAEA;;AACA,UAAA,CAAA,GAAA,IAAA,CAAA,UAAA,EAAA,CAHA,CAGA;;AACA,UAAA,CAAA,GAAA,IAAA,CAAA,UAAA,EAAA,CAJA,CAIA;;AACA,UAAA,MAAA,GAAA,CAAA,KAAA,EAAA,KAAA,EAAA,KAAA,EAAA,KAAA,EAAA,KAAA,EAAA,KAAA,EAAA,KAAA,EAAA,KAAA,EAAA,KAAA,EAAA,KAAA,EAAA,KAAA,EAAA,KAAA,CAAA;AACA,UAAA,IAAA,GAAA,CAAA,KAAA,EAAA,KAAA,EAAA,KAAA,EAAA,KAAA,EAAA,KAAA,EAAA,KAAA,EAAA,KAAA,CAAA,CANA,CAOA;;AACA,UAAA,OAAA,GAAA,